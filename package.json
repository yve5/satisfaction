{
  "name": "satisfaction",
  "version": "1.0.1",
  "private": true,
  "homepage": "https://yve5.github.io/satisfaction",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "bootstrap": "5.3.3",
    "date-fns": "3.6.0",
    "jest-canvas-mock": "2.5.2",
    "jspdf": "2.5.2",
    "organe": "1.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "react-scripts": "5.0.1",
    "redux": "5.0.1",
    "redux-mock-store": "1.5.5",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "gh-pages": "6.2.0",
    "jest-watch-typeahead": "0.6.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/{index,reportWebVitals}.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^organe/(.*)$": "<rootDir>/node_modules/organe/lib/$1.cjs.js"
    }
  }
}
